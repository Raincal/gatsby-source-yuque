    "@babel/core": "^7.4.5",
    "babel-preset-gatsby-package": "^0.1.4",
    "cross-env": "^5.2.0"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin",
    "yuque"
