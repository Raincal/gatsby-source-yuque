{
  "name": "gatsby-source-yuque",
  "description": "Source plugin for pulling data into Gatsby from [语雀](https://www.yuque.com) API.",
  "version": "0.0.8",
  "author": "Raincal <cyj94228@gmail.com>",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://github.com/Raincal/gatsby-source-yuque#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Raincal/gatsby-source-yuque"
  },
  "bugs": {
    "url": "https://github.com/Raincal/gatsby-source-yuque/issues"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
